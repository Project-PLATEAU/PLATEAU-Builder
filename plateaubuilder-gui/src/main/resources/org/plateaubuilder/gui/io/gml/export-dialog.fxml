<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.plateaubuilder.gui.io.gml.ExportDialogController">
   <children>
      <VBox alignment="CENTER" spacing="10.0">
          <padding>
              <Insets bottom="10" left="10" right="10" top="10" />
          </padding>
          <Label text="Folder Name:" />
          <TextField fx:id="folderNameTextField" promptText="Folder Name" />
          <Button fx:id="confirmButton" onAction="#handleConfirm" text="Confirm" />

      </VBox>
   </children>
    <stylesheets>
        <URL value="@../../viewer.css" />
    </stylesheets>
</AnchorPane>
