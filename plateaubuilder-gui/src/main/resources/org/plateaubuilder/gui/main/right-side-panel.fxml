            <Tab>
                <graphic>
                    <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/icon_info.png" />
                    </ImageView>
                </graphic>
                <fx:include source="../featureinfo/feature-info.fxml"/>
            </Tab>
            <Tab>
                <graphic>
                    <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/icon_settings.png" />
                    </ImageView>
                </graphic>
                <fx:include source="../settings/view-settings.fxml" />
            </Tab>
            <Tab>
                <graphic>
                    <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/icon_Light.png" />
                    </ImageView>
                </graphic>
                <fx:include source="../settings/light-settings.fxml" />
            </Tab>
            <Tab>
                <graphic>
                    <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/icon_camera.png" />
                    </ImageView>
                </graphic>
                <fx:include source="../settings/camera-settings.fxml" />
            </Tab>
            <Tab>
                <graphic>
                    <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/icon_basemap.png" />
                    </ImageView>
                </graphic>
                <fx:include source="../featureinfo/baseMap-info.fxml"/>
            </Tab>
        </TabPane>
    </VBox>
    <stylesheets>
        <URL value="@../viewer.css" />
    </stylesheets>
</AnchorPane>
