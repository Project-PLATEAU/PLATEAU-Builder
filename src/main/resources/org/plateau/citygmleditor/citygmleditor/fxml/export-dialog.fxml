<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/1" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="10.0"  fx:controller="org.plateau.citygmleditor.citygmleditor.ExportDialogController" >
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <Label text="Folder Name:"/>
    <TextField fx:id="folderNameTextField" promptText="Folder Name"/>
    <Button fx:id="confirmButton" text="Confirm" onAction="#handleConfirm"/>
</VBox>
