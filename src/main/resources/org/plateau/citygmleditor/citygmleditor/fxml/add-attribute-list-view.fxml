<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="addingAttributeMenu" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.plateau.citygmleditor.citygmleditor.AddingAttributeController">
    <!-- 検索ボックス -->
    <TextField fx:id="searchField" promptText="Search"/>
    
    <!-- TableView -->
    <TableView fx:id="attributeTableView" onMouseClicked="#handleMouseClick">
        <columns>
            <TableColumn fx:id="name" text="name" />
            <TableColumn fx:id="description" text="description" />
        </columns>
    </TableView>
</VBox>